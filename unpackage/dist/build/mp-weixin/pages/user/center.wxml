<view class="wrap"><view class="background"></view><view class="head"><view class="hd"><view class="head-img"><image src="{{user.avatarUrl}}" mode="aspectFill"></image></view><view class="info"><view class="name">{{user.nickName}}</view><view class="balance">{{"余额: ¥"+user.balance}}</view></view></view><view class="ft"><view data-event-opts="{{[['tap',[['showPopup',['$event']]]]]}}" class="btn green" bindtap="__e">微信提现</view></view></view><view class="body"><view data-event-opts="{{[['tap',[['navTo',['/pages/user/withdrawlist']]]]]}}" class="item" bindtap="__e"><view class="hd">提现列表</view><view class="ft"><text class="iconfont iconyoujiantou"></text></view></view><view data-event-opts="{{[['tap',[['navTo',['/pages/user/balancelist']]]]]}}" class="item" bindtap="__e"><view class="hd">资金明细</view><view class="ft"><text class="iconfont iconyoujiantou"></text></view></view><view data-event-opts="{{[['tap',[['navTo',['/pages/user/rebatelist']]]]]}}" class="item" bindtap="__e"><view class="hd">返利列表</view><view class="ft"><text class="iconfont iconyoujiantou"></text></view></view><view data-event-opts="{{[['tap',[['navTo',['/pages/user/sublist']]]]]}}" class="item" bindtap="__e"><view class="hd">我的下级</view><view class="ft"><text class="iconfont iconyoujiantou"></text></view></view></view><block wx:if="{{show_popup}}"><view class="withdraw-popup"><view data-event-opts="{{[['tap',[['hidePopup',['$event']]]]]}}" class="mask" bindtap="__e"></view><view class="popup"><view class="title">提现金额</view><input type="number" placeholder="请输入提现金额" data-event-opts="{{[['input',[['__set_model',['','withdraw_amount','$event',[]]]]]]}}" value="{{withdraw_amount}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['withdrawConfirm',['$event']]]]]}}" class="confirm" bindtap="__e">确定</view></view></view></block></view>