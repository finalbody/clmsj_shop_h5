.flex-between{display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;justify-content: space-between;}
.red{color: #FF0000;margin: 0px 2px;}
.pickBox{display: -webkit-box;display: -webkit-flex;display: flex; -webkit-box-pack: end; -webkit-justify-content: flex-end; justify-content: flex-end; -webkit-flex-wrap: wrap; flex-wrap: wrap;}
.pickBox .pickItem{
	margin: 5px 5px; 
	border: 1px solid #D9D9D9; 
	padding: 5px 6px;
	min-width: 40px; 
	border-radius: 5px;
	white-space:nowrap;
}
.pickBox .on{
	background: #F76260;
	border: 1px solid #F76260;
	color: #FFFFFF;
}
.tips{
	padding: 0px;
}
.tips .mask-bg{
	left: 0;right: 0;top: 0;bottom: 0; background: #303133;opacity: 0.5; z-index: 98;position: fixed;
}
.tips .mask-box{
	position: fixed;bottom: calc(50% - 75px); left: 10%;right: 10%;z-index: 99;background: #ffffff;padding: 20px;min-height: 150px;border-radius: 5px;
}
